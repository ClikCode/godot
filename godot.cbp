<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="godot" />
		<Option makefile="scons" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/godot.debug.opts" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<MakeCommands>
					<Build command="scons platform=x11 tools=no target=release_debug bits=32" />
					<CompileFile command="" />
					<Clean command="" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="" />
					<SilentBuild command="scons platform=x11 tools=no target=release_debug bits=32 &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/godot.release.opts" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<MakeCommands>
					<Build command="scons platform=x11 tools=no target=release bits=32" />
					<CompileFile command="" />
					<Clean command="" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="" />
					<SilentBuild command="scons platform=x11 tools=no target=release bits=32 &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Extensions>
			<code_completion>
				<search_path add="." />
				<search_path add="core" />
				<search_path add="core/os" />
				<search_path add="core/math" />
				<search_path add="tools" />
				<search_path add="drivers" />
				<search_path add="platform/x11" />
			</code_completion>
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
